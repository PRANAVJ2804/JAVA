import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int age = sc.nextInt();

        if(age>=21){
            System.out.println("person is major");
        } else if (age>=18) {
            System.out.println("person is grown");
        } else{
            System.out.println("person is minor");
        }
    }
}
